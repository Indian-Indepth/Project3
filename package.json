    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "braintree": "^2.19.0",
    "chart.js": "^2.8.0",
    "concurrently": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
